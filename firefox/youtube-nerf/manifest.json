{

  "manifest_version": 2,
  "name": "YoutubeNerf",
  "version": "2.3",

  "description": "Blocks all recommendations & comments on youtube for firefox, and decontents ig",

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["youtube.js"]
    },
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["instagram.js"]
    }
  ]
}
